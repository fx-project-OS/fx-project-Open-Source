		$fxdst .= '<tr><td colspan="2" align="center"><i class="lightgrey s1"><br />('.$msg.'!)</i></td></tr>'.$GLOBALS['nl'];
	}

	$fxdst .= '</table>'.$GLOBALS['nl'];

	return $fxdst;
