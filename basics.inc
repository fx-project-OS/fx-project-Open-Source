 */
function fxf_getReferenceValues($idf='', $order='', $ordpos=false)
{
	// Display function call?
	if($GLOBALS['__debug']['debugmode']&FXP_DEBUG_FCT) { fxDebugFunction(); }

	$idf=strtolower(trim($idf));
	switch($idf)
	{
		case 'priority':
		case 'prioritaet':
			$idf='47';
		break;

		case 'projectstatus':
		case 'projektstatus':
		case 'project_status':
		case 'projekt_status':
			$idf='137';
		break;

		case 'method':
		case 'methode':
			$idf='1659';
		break;

		case 'pointintime':
		case 'zeitpunkt':
			$idf='1660';
		break;

		case 'persontype':
		case 'personentyp':
			$idf='30';
		break;

		case 'language':
		case 'sprache':
			$idf='53';
		break;

		case 'day':
		case 'tag':
		case 'days':
		case 'tage':
		case 'weekday':
		case 'wochentag':
		case 'weekdays':
		case 'wochentage':
			$idf='289';
			$ordpos=true;
		break;

		case 'month':
		case 'monat':
		case 'months':
		case 'monate':
			$idf='62';
			$ordpos=true;
		break;

		case 'communicationtype':
		case 'kommunikationstyp':
			$idf='94';
		break;

		case 'category':
		case 'kategorie':
			$idf='1,10';
		break;
	}

	if(!strlen($idf))
		return '';

	$ie=explode(',',$idf);
	$ids='';
	foreach($ie as $i)
	{
		$ids .= ','.(int)$i;
		if((int)$i == 2049)	// Abrechnungsart_Mat -> Abrechnungsart
			$ids .= ',69';
	}
	$ids=substr($ids,1);

	$sql="SELECT mandanten_id, wertetabid, tabwert, position FROM wertetab WHERE mandanten_id IN (0,".$GLOBALS['fxpglobals']['client'].") AND id_feld IN (".$ids.") AND id_sprache=".$GLOBALS['fxpglobals']['lang']." AND (archiv IS NULL OR archiv=0)";
	if(strlen($order))
		$sql .= db_orderby($order);
	else
		$sql .= db_orderby('position', 'tabwert');
	if($ordpos)
		$arr=get_array($sql, 'position', 'tabwert');
	else
		$arr=get_array($sql, 'wertetabid', 'tabwert');
//fxDebug($arr, $sql);

	return $arr;
}

/**
 * Create an array with the proper value for each bit
 *
 * @param integer $mb - Mandatory parameter:  Max bits
 *
 * @return Array of bit values
 */
function fxf_getBitArray($mb=8)
{
	// Display function call?
	if($GLOBALS['__debug']['debugmode']&FXP_DEBUG_FCT) { fxDebugFunction(); }

	$mb=max(1, min(128, (int)$mb));
	$ba=array();

	for($b=0; $b<$mb; $b++)
		$ba[$b]=pow(2,$b);

	return $ba;
}

