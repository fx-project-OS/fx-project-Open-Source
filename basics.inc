function fxpALink($href, $download='', $text='', $style='', $target='_blank')
{
	// Display function call?
	if($GLOBALS['__debug']['debugmode']&FXP_DEBUG_FCT) { fxDebugFunction(); }

	$l='<a href="'.$href;
	$qp=strpos($href,'?');
	if($qp === false)
		$l .= '?';
	else
		$l .= '&';
	$l .= substr(fxSetSession(),1).'"';
